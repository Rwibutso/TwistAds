  "testTimeout": 10000,
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.css$": "jest-transform-css",
    "\\.js$": "<rootDir>/node_modules/babel-jest",
    "^.+\\.(js|ts)$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(react-qr-reader)/)"
  ],
  "scripts": {
    "test": "react-scripts test --transformIgnorePatterns --coverage --timeout=15000 --runInBand",
